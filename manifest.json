{
    "name": "Facebook Intervention",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.facebook.com/*",
            "http://www.facebook.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Interactions"
    },
    "background": {
      "scripts": ["background.js"]
    },
  "permissions": ["tabs"]
}
